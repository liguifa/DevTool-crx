{
	"name":"程序员小工具",
	"version":"1.0.0",
	"manifest_version": 2,  
	"description":"程序员Tool集合",
	"browser_action": 
	{  
    	"default_icon": "res/icon_48.png" ,
    	"default_title": "程序员小工具"
    },
    "icons":
    {
        "16": "res/icon_16.png",             
        "48": "res/icon_48.png",            
        "128": "res/icon_128.png"   
    },
	"content_scripts": 
	[{
    	"matches": ["https://*/*","http://*/*"],
    	"js": ["app.js"]
    }] 
}